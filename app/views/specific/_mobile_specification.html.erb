	<td id="spec-value"><%=  @mobile_multimedia_audiojack.gsub("check quality","").titlecase %></td> 
</tr>

<tr id="spec-row">
	<td id="spec-name">Speaker:</td>
	<td id="spec-value"><%= @mobile_multimedia_speaker.titlecase %></td> 
</tr>

<tr>
	<td id="spec-name" colspan=2 style="text-align:left;padding-left:10px">DATA AND CONNECTIVITY</td>
</tr>

<tr id="spec-row">
	<td id="spec-name">Messaging:</td>
	<td id="spec-value"><%=  @mobile_data_messaging.titlecase %></td> 
</tr>

<tr id="spec-row">
	<td id="spec-name">Browser Support:</td>
	<td id="spec-value">
	<% if @mobile_data_browser_support.chomp.downcase.gsub(".","")=="na"%>
		No
	<%else%>
		<%=  @mobile_data_browser_support.titlecase  %>
	<%end%>
	</td> 
</tr>

<tr id="spec-row">
	<td id="spec-name">2G:</td>
	<td id="spec-value"><%=  @mobile_data_2g_value.upcase %></td> 
</tr>

