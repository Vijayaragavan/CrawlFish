	<td id="spec-name">GPRS:</td>
	<td id="spec-value"><%= @mobile_data_gprs_value.titlecase %></td> 
</tr>

<tr id="spec-row">
	<td id="spec-name">EDGE:</td>
	<td id="spec-value"><%= @mobile_data_edge_value.titlecase %></td> 
